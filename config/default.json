{
  "cps": {
    "baseUrl": "http://192.168.118.23:9000",
    "timeout": 15000,
    "wlServiceDomain": "maaii.com",
    "sdkServiceDomain": "m800-api.com",
    "chargeProfile": {
      "company": "m800_charge_profile",
      "user": "maaii_charge_profile"
    },
    "sip": {
      "gateway": {
        "timesToGenerate": 2,
        "template": {
          "identifier": "{{ carrierId }}.gateway.tsbc{{ generateNumber }}",
          "description": "Transcoding SBC #{{ generateNumber }}",
          "host": "192.168.35.50",
          "port": 5080,
          "manipulation_rules": {
            "0": {
              "description": "NGN Header to Prefix Manipulation",
              "matcher": {
                "type": "RegExField",
                "description": "For all numbers",
                "field_name": "To",
                "regular_expression": "^((:?tel:)?\\+?)([1-9]\\d+)$"
              },
              "manipulator": {
                "type": "OffNetCall",
                "description": "Replace From Header With Anonymous",
                "from_address": "anonymous",
                "gateway_prefix": "+00000000",
                "enabled": false,
                "is_passerted_id_enabled": false,
                "is_one_card_multiple_no": false
              }
            }
          }
        }
      },
      "routing": {
        "template": {
          "identifier" : "{{ carrierId }}.profile",
          "description": "For {{ carrierId }} carrier",
          "trunks" : {
            "0": {
              "matchers" : {
                "0": {
                  "type": "RegExField",
                  "description" : "Select caller Only Within Domain {{ carrierId }}",
                  "field_name": "From",
                  "regular_expression": "^((:?sip:)?\\+{0,1})(.*[A-Za-z_]+.*)@{{ escapedCarrierId }}$"
                }
              },
              "gateway_selection_rules": {
                "0": {
                  "description": "Select caller Only Within Domain {{ carrierId }}",
                  "matcher": {
                    "type": "RegExField",
                    "description": "Select caller Only Within Domain {{ carrierId }}",
                    "field_name": "From",
                    "regular_expression": "^((:?sip:)?\\+{0,1})(.*[A-Za-z_]+.*)@{{ escapedCarrierId }}$"
                  },
                  "gateway": "{{ sipGatewayId1 }}"
                },
                "1": {
                  "description": "Select caller Only Within Domain {{ carrierId }}",
                  "matcher": {
                    "type": "RegExField",
                    "description": "Select caller Only Within Domain {{ carrierId }}",
                    "field_name": "From",
                    "regular_expression": "^((:?sip:)?\\+{0,1})(.*[A-Za-z_]+.*)@{{ escapedCarrierId }}$"
                  },
                  "gateway": "{{ sipGatewayId2 }}"
                }
              }
            }
          }
        }
      }
    }
  },
  "iam": {
    "baseUrl": "http://deploy.dev.maaii.com:4004"
  },
  "mongo": {
    "uri": "mongodb://localhost:27017/maaii-provisioning-service"
  }
}
