{
  "name": "maaii-provisioning-service",
  "private": true,
  "version": "1.0.0",
  "description": "A microservice for Service Provisioning",
  "main": "index.js",
  "scripts": {
    "prestart": "npm run build",
    "start": "npm run server",
    "dev": "nodemon --exec 'babel-node src/server' --watch src --ext js,jsx,json",
    "server": "node build/server",
    "build": "npm-run-all build:js build:bpmn",
    "build:js": "babel src --out-dir build --source-maps",
    "build:bpmn": "cp src/processes/provisioning/index.bpmn build/processes/provisioning/index.bpmn",
    "lint": "eslint src/**/*.js",
    "test": "mocha --compilers js:babel-core/register --recursive test",
    "coverage": "nyc npm test",
    "doc:api": "apidoc -i src/server/ -o doc/api/",
    "jenkins": "npm prune && npm --verbose i",
    "prejenkins:commit": "npm run jenkins",
    "jenkins:commit": "npm run lint && npm run build",
    "prejenkins:merge": "npm run jenkins",
    "jenkins:merge": "npm run build"
  },
  "nyc": {
    "check-coverage": true,
    "lines": 90,
    "statements": 90,
    "functions": 100,
    "branches": 80,
    "include": [
      "src/**/*.js"
    ],
    "exclude": [
      "src/processes/**/*.js",
      "src/server/**/*.js",
      "src/initializer/**/*.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "ssh://georgejor@gerrit.dev.maaii.com:29418/maaii-provisioning-service"
  },
  "devDependencies": {
    "apidoc": "^0.16.1",
    "babel-cli": "^6.10.1",
    "babel-core": "^6.10.4",
    "babel-eslint": "^6.1.2",
    "babel-plugin-istanbul": "^1.0.3",
    "babel-plugin-transform-export-extensions": "^6.8.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-2": "^6.11.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^5.3.0",
    "eslint": "^2.13.1",
    "eslint-config-airbnb-base": "^3.0.1",
    "eslint-plugin-import": "^1.9.2",
    "mocha": "^2.5.3",
    "npm-run-all": "^2.3.0",
    "nyc": "^7.1.0-candidate3",
    "supertest": "^1.2.0"
  },
  "dependencies": {
    "babel-polyfill": "^6.9.1",
    "bluebird": "^3.4.1",
    "body-parser": "^1.15.2",
    "bottlejs": "^1.3.0",
    "bpmn": "^0.2.2",
    "common-errors": "^0.5.7",
    "express": "^4.14.0",
    "faker": "^3.1.0",
    "joi": "^9.0.4",
    "lodash": "^4.13.1",
    "m800-health-check": "^0.3.2",
    "m800-initializers": "^0.1.2",
    "m800-winston": "^0.1.1",
    "method-override": "^2.3.6",
    "mongoose": "^4.5.3",
    "mongoose-timestamp": "^0.6.0",
    "mongoose-unique-validator": "^1.0.2",
    "morgan": "^1.7.0",
    "nodemon": "^1.9.2",
    "sinon": "^1.17.4",
    "source-map-support": "^0.4.2",
    "superagent": "^2.0.0",
    "superagent-bluebird-promise": "^3.0.2",
    "uuid": "^2.0.2",
    "winston": "^2.2.0"
  }
}
