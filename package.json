{
  "name": "maaii-provisioning-service",
  "private": true,
  "version": "1.2.0",
  "description": "A microservice for Service Provisioning",
  "main": "index.js",
  "scripts": {
    "prestart": "npm run build",
    "start": "npm run server",
    "dev": "gulp dev",
    "watch": "gulp watch",
    "predebug": "npm run build",
    "debug": "node debug --inspect=5858 build/src",
    "server": "node build/src",
    "build": "gulp build && gulp compileMigration",
    "docs": "gitbook install && gitbook build && npm run docs:api",
    "docs:api": "m800-swagger-docs-generator -i src/api/swagger.yaml -o _apidocs",
    "lint": "gulp lint",
    "test": "gulp test",
    "coverage": "nyc --reporter=html --reporter=lcov --reporter=text --reporter=text-summary npm run test",
    "jenkins": "npm prune && npm --verbose i",
    "prejenkins:commit": "npm run jenkins",
    "jenkins:commit": "npm run coverage && npm run build -- --env=production",
    "prejenkins:merge": "npm run jenkins",
    "jenkins:merge": "npm run jenkins:commit && npm run docs",
    "migrate": "cd build/migrations && migrate-mongo up && cd ../../"
  },
  "nyc": {
    "check-coverage": false,
    "lines": 90,
    "statements": 90,
    "functions": 100,
    "branches": 80,
    "include": [
      "src/**/*.js"
    ],
    "exclude": [
      "src/util/formatError.js",
      "src/util/checkArgument.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "ssh://georgejor@gerrit.dev.maaii.com:29418/maaii-provisioning-service"
  },
  "devDependencies": {
    "babel-core": "^6.10.4",
    "babel-eslint": "^6.1.2",
    "babel-plugin-istanbul": "^3.0.0",
    "babel-plugin-transform-export-extensions": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-stage-2": "^6.17.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^5.3.0",
    "chalk": "^1.1.3",
    "eslint": "^2.13.1",
    "eslint-config-airbnb-base": "^3.0.1",
    "eslint-plugin-import": "^1.9.2",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-changed": "^1.3.2",
    "gulp-changed-in-place": "^2.0.3",
    "gulp-eslint": "^3.0.1",
    "gulp-mocha": "^3.0.1",
    "gulp-nodemon": "^2.2.1",
    "gulp-sourcemaps": "^2.0.0-alpha",
    "m800-swagger-docs-generator": "^0.1.0",
    "minimist": "^1.2.0",
    "mocha": "^2.5.3",
    "nyc": "^10.0.0",
    "sinon": "^1.17.4",
    "superagent-proxy": "^1.0.1",
    "supertest": "^1.2.0"
  },
  "dependencies": {
    "babel-polyfill": "^6.9.1",
    "bluebird": "^3.4.1",
    "body-parser": "^1.15.2",
    "bottlejs": "^1.3.0",
    "bpmn": "^0.2.2",
    "common-errors": "^0.5.7",
    "escape-string-regexp": "^1.0.5",
    "express": "^4.14.0",
    "faker": "^3.1.0",
    "joi": "^9.0.4",
    "left-pad": "^1.1.1",
    "lodash": "^4.13.1",
    "lodash-deep": "^2.0.0",
    "m800-health-check": "^0.3.2",
    "m800-initializers": "^0.1.2",
    "m800-prometheus-express": "^0.1.0",
    "m800-util": "^0.7.0-snapshot.102",
    "m800-winston": "^0.1.1",
    "method-override": "^2.3.6",
    "migrate-mongo": "^1.1.0",
    "mongoose": "^4.7.5",
    "mongoose-timestamp": "^0.6.0",
    "mongoose-unique-validator": "^1.0.2",
    "morgan": "^1.7.0",
    "multer": "^1.2.1",
    "nconf": "^0.8.4",
    "nodemon": "^1.9.2",
    "serialize-error": "^2.0.0",
    "shallowequal": "^0.2.2",
    "source-map-support": "^0.4.2",
    "superagent": "^2.0.0",
    "superagent-bluebird-promise": "^3.0.2",
    "superagent-logger": "^1.1.0",
    "superagent-throttle": "^0.1.1",
    "uuid": "^2.0.2",
    "winston": "^2.2.0"
  }
}
